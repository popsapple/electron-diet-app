{
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "dist/**/*.js",
      "*.js"
    ]
  },"rules": {
    "object-literal-sort-keys": false,
    "curly": false,
    "no-shadowed-variable": false,
    "one-variable-per-declaration": false,
    "no-empty": false,
    "no-conditional-assignment": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "curly": false,
    "no-shadowed-variable": false,
    "one-variable-per-declaration": false,
    "no-empty": false,
    "no-conditional-assignment": false
  }
}
